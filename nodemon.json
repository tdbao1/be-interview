{
  "restartable": "rs",
  "ignore": [
      ".git",
      "node_modules",
      "node_modules/**/node_modules",
      "src/__generated__",
      "*.test.ts"
  ],
  "verbose": true,
  "execMap": {
      "ts": "node --require ts-node/register"
  },
  "watch": [
      ".env",
      "src/"
  ],
  "env": {
      "NODE_ENV": "development"
  },
  "ext": "js,json,ts,graphql",
  "delay": "3"
}